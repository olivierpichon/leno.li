{
  "name": "leno.li",
  "version": "1.0.0",
  "description": "A photo album website",
  "main": "index.js",
  "config": {
    "s3BucketName": "lenoli",
    "region": "ap-southeast-2",
    "cloudFormationStackName": "lenoli",
    "functionName": "lenoli",
    "accountId": "083835757174"
  },
  "scripts": {
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js",
    "create-bucket": "aws s3 mb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "delete-bucket": "aws s3 rb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "package": "aws cloudformation package --template ./cloudformation.yaml --s3-bucket $npm_package_config_s3BucketName --output-template packaged-sam.yaml --region $npm_package_config_region",
    "deploy": "aws cloudformation deploy --template-file packaged-sam.yaml --stack-name $npm_package_config_cloudFormationStackName --capabilities CAPABILITY_IAM --region $npm_package_config_region",
    "package-deploy": "npm run build && npm run package && npm run deploy",
    "delete-stack": "aws cloudformation delete-stack --stack-name $npm_package_config_cloudFormationStackName --region $npm_package_config_region",
    "setup": "npm install && (aws s3api get-bucket-location --bucket $npm_package_config_s3BucketName --region $npm_package_config_region || npm run create-bucket) && npm run package-deploy",
    "clean": "rm -rf ./dist && rm -f ./compiled_server.js ./api-gateway-event.json ./rm packaged-sam.yaml",
    "build-server": "./node_modules/webpack/bin/webpack.js --progress --color -p --config webpack.server.js",
    "build-client": "./node_modules/webpack/bin/webpack.js --progress --color -p --config webpack.prod.js",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "start": "node index.js",
    "start-dev": "nodemon -e js,jsx index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Redux",
    "React",
    "Isomorphic",
    "ES6",
    "Love"
  ],
  "author": "Olivier Pichon",
  "license": "MIT",
  "dependencies": {
    "aws-serverless-express": "^3.0.1",
    "axios": "^0.16.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "blob-to-buffer": "^1.2.6",
    "buffer": "^5.0.6",
    "css-loader": "^0.28.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "google-auth-library": "git+https://github.com/olivierpichon/google-auth-library.git",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "object-assign": "^4.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-images": "^0.5.4",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-transition-group": "^1.1.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.1",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "react-transform-hmr": "^1.0.4",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-node-externals": "^1.6.0"
  }
}
